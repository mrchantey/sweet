{
	"rust-analyzer.cargo.features": ["sweet/bevy"],
	"rust-analyzer.cargo.target": "wasm32-unknown-unknown",
	"deno.enable": true,
	"search.exclude": {
		"**/node_modules": true,
		"**/bower_components": true,
		"**/*.code-search": true,
		"**/mod.rs": true,
		"target/**/*": true,
	},
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		"**/mod.rs": true,
	}
}